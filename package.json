{
  "name": "mundo-da-lua-home",
  "version": "1.0.0",
  "description": "sass --watch sass/style.scss:css/style.css",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run sass:watch\" \"next dev\"",
    "sass:watch": "sass --watch sass/style.scss styles/style.css",
    "sass:build": "sass sass/style.scss styles/style.css && sass sass/style.scss styles/style.min.css --style=compressed",
    "build": "npm run sass:build && next build",
    "start": "next start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "next": "^15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
